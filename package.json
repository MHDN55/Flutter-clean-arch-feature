{
  "name": "flutter-clean-architecture-helper",
  "displayName": "Flutter Clean Architecture Helper",
  "description": "A VS Code extension to create feature folders and use cases for Flutter Clean Architecture projects.",
  "version": "1.1.0",
  "publisher": "your-publisher-id",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [
      "onCommand:flutterExtension.createUseCase",
      "onCommand:flutterExtension.createFeatureFolders"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "flutterExtension.createUseCase",
              "title": "Create Use Case",
              "category": "Arch"
          },
          {
              "command": "flutterExtension.createFeatureFolders",
              "title": "Create Feature Folders",
              "category": "Arch"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./"
  },
  "devDependencies": {
      "@types/vscode": "^1.60.0",
      "typescript": "^4.4.3"
  }
}
