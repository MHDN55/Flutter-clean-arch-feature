{
    "name": "flutter-clean-arch-feature",
    "displayName": "Flutter clean arch feature",
    "description": "A VS Code extension to create feature folders and use cases for Flutter Clean Architecture projects.",
    "version": "1.2.0",
    "publisher": "MHDN",
    "engines": {
      "vscode": "^1.60.0"
    },
    "categories": [
      "Other"
    ],
    "activationEvents": [
      "onCommand:flutterExtension.createUseCase",
      "onCommand:flutterExtension.createFeatureFolders",
      "onCommand:flutterExtension.createDomainRepository"
    ],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "flutterExtension.createUseCase",
          "title": "Create Use Case",
          "category": "Arch"
        },
        {
          "command": "flutterExtension.createFeatureFolders",
          "title": "Create Feature Folders",
          "category": "Arch"
        },
        {
          "command": "flutterExtension.createDomainRepository",
          "title": "Create Domain Repository",
          "category": "Arch"
        }
      ]
    },
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./"
    },
    "devDependencies": {
      "@types/vscode": "^1.60.0",
      "typescript": "^4.4.3"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/MHDN55/Flutter-clean-arch-feature"
    },
    "bugs": {
      "url": "https://github.com/MHDN55/Flutter-clean-arch-feature/issues"
    },
    "homepage": "https://github.com/MHDN55/Flutter-clean-arch-feature#readme"
  }
  